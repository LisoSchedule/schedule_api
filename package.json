{
  "name": "liso-schedule-api",
  "version": "1.0.0",
  "description": "RESTful API for managing university schedules, assignments, and reminders, integrated with a Telegram bot interface",
  "author": {
    "name": "Andrew Kononenko [wastardy]",
    "email": "wastardy.k@gmail.com",
    "url": "https://github.com/wastardy"
  },
  "license": "MIT",
  "main": "server.js",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^7.5.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-alias": "^1.2.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^6.6.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  },
  "scripts": {
    "build": "tsc",
    "db:reset": "prisma migrate reset",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "dev": "tsx watch src/index.ts",
    "dev:docker": "docker-compose -f docker/docker-compose.dev.yml --env-file .env -p liso-schedule-dev up -d",
    "dev:docker:stop": "docker-compose -f docker/docker-compose.dev.yml --env-file .env -p liso-schedule-dev stop",
    "dev:docker:down": "docker-compose -f docker/docker-compose.dev.yml --env-file .env -p liso-schedule-dev down",
    "dev:docker:logs": "docker-compose -f docker/docker-compose.dev.yml --env-file .env -p liso-schedule-dev logs -f",
    "dev:docker:reset": "docker-compose -f docker/docker-compose.dev.yml --env-file .env -p liso-schedule-dev down -v",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:debug": "eslint . --debug",
    "lint:fix": "eslint . --fix",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:show": "prisma studio",
    "seed": "tsx prisma/seed.ts",
    "start": "node build/src/index.js",
    "start:dev": "npm run dev:docker && npm run dev",
    "test": "prisma generate && jest --coverage"
  },
  "keywords": [
    "node",
    "express",
    "typescript",
    "rest",
    "api",
    "postgres",
    "prisma",
    "docker",
    "telegram",
    "university",
    "schedule",
    "reminders"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LisoSchedule/schedule_api.git"
  }
}
